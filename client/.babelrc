{
  "presets": [
    "@babel/preset-env",
    [
      "@babel/preset-react", {
        "runtime": "automatic"
      }
    ]
  ],
  "plugins": [
    "dynamic-import-webpack",
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": false,
        "helpers": false,
        "regenerator": true,
        "useESModules": false
      }
    ],
    "@babel/plugin-transform-regenerator"
  ]
}
